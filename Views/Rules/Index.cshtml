@model GoonGamesOuh.Models.Rules;
@{
	ViewData["Title"] = "Rules";
	if (Model.LoginStatus)
	{
		ViewData["LoginStatus"] = "Logout";
	}
	else
	{
		ViewData["LoginStatus"] = "Login/Register";
	}
}
<div class="paper-screen">
	<div class="paper-text">
        <ol>
            @{
                foreach (string rule in Model.rules)
                {
                    <li>@Html.Raw(rule)</li>
					<hr />
                } 
			}
        </ol>
	</div>
</div>